  # Netfox - request network viewer
  pod 'netfox', '1.21.0'

  # Reactive
  pod 'RxSwift', '6.5.0'

  #Snapkit UI
  pod 'SnapKit', '~> 5.6.0'

  target 'MiniMoviesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MiniMoviesUITests' do
    # Pods for testing
  end

end
